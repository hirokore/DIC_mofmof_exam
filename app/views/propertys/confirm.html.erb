<h1>確認画面</h1>
<%= form_with(model: @property, url: propertys_path, local: true) do |f| %>
  <%= f.hidden_field :name %>
  <p>物件名：<%= @property.name %></p>
  <%= f.hidden_field :value %>
  <p>賃料：<%= @property.value.to_s(:delimited) %>円</p>
  <%= f.hidden_field :address %>
  <p>住所：<%= @property.address%></p>
  <%= f.hidden_field :age %>
  <p>築年数：<%= @property.age%>年</p>
  <%= f.hidden_field :memo %>
  <p>備考：<%= @property.memo%></p> 
  <%= f.submit "投稿する" %>
<% end %>
<%= form_with(model:@property, url:new_property_path, local:true, method:"get") do |f| %>
  <%= f.hidden_field :name %>
  <%= f.hidden_field :value %>
  <%= f.hidden_field :address %>
  <%= f.hidden_field :age %>
  <%= f.hidden_field :memo %>
  <%= f.submit "戻る", name: "back" %>
<% end %>